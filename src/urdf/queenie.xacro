<?xml version='1.0'?>

<robot name="queenie" xmlns:xacro="http://www.ros.org/wiki/xacro">

  <xacro:include filename="$(find queenie)/src/urdf/queenie.gazebo" />
  <xacro:include filename="$(find queenie)/src/urdf/queenie.trans" />
  <!-- <xacro:include filename="$(find queenie)/urdf/queenie.trans"  /> -->

  <xacro:macro name="default_inertial" params="mass">
    <inertial>
      <mass value="${mass}" />
      <inertia ixx="1.0" ixy="0.0" ixz="0.0" iyy="1.0" iyz="0.0" izz="1.0" />
    </inertial>
  </xacro:macro>

  <xacro:macro name="default_inertial_cuboid" params="mass x_length y_length z_length x:=0 y:=0 z:=0 r:=0 p:=0 yaw:=0">
    <inertial>
      <!-- <xacro:insert_block name="origin"/> -->
      <origin xyz="${x} ${y} ${z}" rpy="${r} ${p} ${yaw}"/>
      <mass value="${mass}" />
      <inertia ixx="${(1/12) * mass * (y_length*y_length + z_length*z_length) * 10}" ixy="0.0" ixz="0.0"
               iyy="${(1/12) * mass * (x_length*x_length + z_length*z_length) * 10}" iyz="0.0"
               izz="${(1/12) * mass * (x_length*x_length + y_length*y_length) * 10}" />
    </inertial>
  </xacro:macro>

  <xacro:macro name="default_inertial_cylinder" params="mass radius height x:=0 y:=0 z:=0 r:=0 p:=0 yaw:=0">
    <inertial>
      <origin xyz="${x} ${y} ${z}" rpy="${r} ${p} ${yaw}"/>
      <mass value="${mass}" />
      <inertia ixx="${(1/12) * mass * (3*radius*radius + height*height) * 10}" ixy="0.0" ixz="0.0"
               iyy="${(1/12) * mass * (3*radius*radius + height*height) * 10}" iyz="0.0"
               izz="${(1/2) * mass * radius*radius * 10}" />
    </inertial>
  </xacro:macro>





  <link name="robot_footprint"></link>

  <joint name="robot_footprint_joint" type="fixed">
    <origin xyz="0 0 0" rpy="0 0 0" />
    <parent link="robot_footprint"/>
    <child link="chassis" />
  </joint>

  <link name='chassis'>
    <xacro:default_inertial_cylinder mass="15" radius="0.25" height="0.2" x="0" y="0" z="0.05" r="0" p="0" yaw="0"/>
    <collision name='collision'>
      <origin xyz="0 0 0.05" rpy=" 0 0 0"/> 
      <geometry>
        <cylinder length="0.2" radius="0.25"/>
      </geometry>
    </collision>
    <visual name='chassis_visual'>
      <origin xyz="0 0 0.05" rpy=" 0 0 0"/>
      <geometry>
       <cylinder length="0.2" radius="0.25"/>
      </geometry>
    </visual>
    <collision name='back_caster_collision'>
      <origin xyz="-0.15 0 -0.05" rpy=" 0 0 0"/>
      <geometry>
        <sphere radius="0.0499"/>
      </geometry>
    </collision>
    <visual name='back_caster_visual'>
      <origin xyz="-0.15 0 -0.05" rpy=" 0 0 0"/>
      <geometry>
        <sphere radius="0.05"/>
      </geometry>
    </visual>
    <collision name='front_caster_collision'>
      <origin xyz="0.15 0 -0.05" rpy=" 0 0 0"/>
      <geometry>
        <sphere radius="0.0499"/>
      </geometry>
    </collision>
    <visual name='front_caster_visual'>
      <origin xyz="0.15 0 -0.05" rpy=" 0 0 0"/>
      <geometry>
        <sphere radius="0.05"/>
      </geometry>
    </visual>
  </link>



  <joint name="gripper_extension" type="fixed">
    <parent link="chassis"/>
    <child link="gripper_pole"/>
    <!-- <limit effort="1000.0" lower="-0.13" upper="0" velocity="0.05"/> -->
    <origin rpy="0 0 0" xyz="0.30 0 0"/>
  </joint>


  <link name="gripper_pole">
    <visual>
      <geometry>
        <box size="0.1 0.05 0.05"/>
      </geometry>
      <origin rpy="0 ${0} 0 " xyz="0 0 0"/>
    </visual>
    <collision>
      <geometry>
         <box size="0.1 0.05 0.05"/>
      </geometry>
      <origin rpy="0 ${0} 0 " xyz="0 0 0"/>
    </collision>
    <xacro:default_inertial_cuboid mass="0.5" x_length="0.1" y_length="0.05" z_length="0.05" x="0" y="0" z="0" r="0" p="0" yaw="0"/>
  </link>

  <joint name="gripper_extension_vertical" type="fixed">
    <parent link="gripper_pole"/>
    <child link="gripper_pole_vertical"/>
    <!-- <limit effort="1000.0" lower="-0.13" upper="0" velocity="0.5"/> -->
    <origin rpy="0 0 0" xyz="0.075 0 0"/>
  </joint>

  <link name="gripper_pole_vertical">
    <visual>
      <geometry>
        <box size="0.05 0.05 0.2"/>
      </geometry>
      <origin rpy="0 0 0 " xyz="0 0 0.075"/>
    </visual>
    <collision>
      <geometry>
        <box size="0.05 0.05 0.2"/>
      </geometry>
      <origin rpy="0 0 0 " xyz="0 0 0.075"/>
    </collision>
    <xacro:default_inertial_cuboid mass="0.3" x_length="0.05" y_length="0.05" z_length="0.2" x="0" y="0" z="0.075" r="0" p="0" yaw="0"/>
  </link>

  <joint name="palm_riser" type="prismatic">
    <parent link="gripper_pole_vertical"/>
    <child link="palm"/>
    <limit effort="1000.0" lower="0" upper="0.17" velocity="0.05"/>
    <origin rpy="0 0 0" xyz="0.025 0 0"/>
    <axis xyz="0 0 1"/>
    <dynamics damping="10.0" friction="1.0" />
  </joint>

  <link name="palm">
      <collision name="collision">
        <geometry>
          <box size="0.05 0.25 0.05"/>
        </geometry>
        <origin rpy="0 0 0 " xyz="0.025 0 0"/>
      </collision>
      <visual name="visual">
        <geometry>
          <box size="0.05 0.25 0.05"/>
        </geometry>
        <origin rpy="0 0 0 " xyz="0.025 0 0"/>
      </visual>
      <xacro:default_inertial_cuboid mass="0.8" x_length="0.05" y_length="0.25" z_length="0.05" x="0.025" y="0" z="0"/>
  </link>

  <joint name="palm_left_finger" type="prismatic">
    <child link="left_finger"/>
    <parent link="palm"/>
    <limit effort="1000.0" lower="0" upper="0.1" velocity="0.5"/>
    <origin rpy="0 0 0" xyz="0.125 0.025 0"/>
    <axis xyz="0 1 0"/>
    <dynamics damping="0.1" friction="1.0" />
  </joint>

  <link name="left_finger">
    <collision name="collision">
      <geometry>
        <box size="0.15 0.05 0.05"/>
      </geometry>
      <origin xyz="0 0 0" rpy="0 0 0 "/>
    </collision>
    <visual name="visual">
      <geometry>
        <box size="0.15 0.05 0.05"/>
      </geometry>
      <origin xyz="0 0 0" rpy="0 0 0 "/>
    </visual>
    <xacro:default_inertial_cuboid mass="0.3" x_length="0.15" y_length="0.05" z_length="0.05" x="0" y="0" z="0"/>
  </link>


  <joint name="palm_right_finger" type="prismatic">
    <child link="right_finger"/>
    <parent link="palm"/>
    <limit effort="1000.0" lower="0" upper="0.1" velocity="0.5"/>
    <origin rpy="0 0 0" xyz="0.125 -0.025 0"/>
    <axis xyz="0 -1 0"/>
    <dynamics damping="0.1" friction="1.0" />
  </joint>

  <link name="right_finger">
    <collision name="collision">
      <geometry>
        <box size="0.15 0.05 0.05"/>
      </geometry>
      <origin xyz="0 0 0" rpy="0 0 0 "/>
    </collision>
    <visual name="visual">
      <geometry>
        <box size="0.15 0.05 0.05"/>
      </geometry>
      <origin xyz="0 0 0" rpy="0 0 0 "/>
    </visual>
    <xacro:default_inertial_cuboid mass="0.3" x_length="0.15" y_length="0.05" z_length="0.05" x="0" y="0" z="0"/>
  </link>
  
  

  

  

  <joint type="continuous" name="left_wheel_hinge">
    <origin xyz="0 0.30 0" rpy="0 0 0" />
    <parent link="chassis"/>
    <child link="left_wheel" />
    <axis xyz="0 1 0" rpy="0 0 0" />
    <limit effort="1000000" velocity="1000" />
    <dynamics damping="1.0" friction="1.0" />
  </joint>

  <link name="left_wheel">
    <xacro:default_inertial_cylinder mass="2" radius="0.1" height="0.05" x="0" y="0" z="0" r="0" p="${pi/2}" yaw="${pi/2}"/>

    <collision>
      <origin xyz="0 0 0" rpy="0 1.5707 1.5707"/>
      <geometry>
        <cylinder radius="0.1" length="0.05"/>
      </geometry>
    </collision>

    <visual>
      <origin xyz="0 0 0" rpy="0 1.5707 1.5707"/>
      <geometry>
        <cylinder radius="0.1" length="0.05"/>
      </geometry>
    </visual>

  </link>

  <joint type="continuous" name="right_wheel_hinge">
    <origin xyz="0 -0.30 0" rpy="0 0 0" />
    <parent link="chassis"/>
    <child link="right_wheel" />
    <axis xyz="0 1 0" rpy="0 0 0" />
    <limit effort="1000000" velocity="1000" />
    <dynamics damping="1.0" friction="1.0" />
  </joint>

  <link name="right_wheel">
    <xacro:default_inertial_cylinder mass="2" radius="0.1" height="0.05" x="0" y="0" z="0" r="0" p="${pi/2}" yaw="${pi/2}"/>

    <collision>
      <origin xyz="0 0 0" rpy="0 1.5707 1.5707"/>
      <geometry>
        <cylinder radius="0.1" length="0.05"/>
      </geometry>
    </collision>

    <visual>
      <origin xyz="0 0 0" rpy="0 1.5707 1.5707"/>
      <geometry>
        <cylinder radius="0.1" length="0.05"/>
      </geometry>
    </visual>

  </link>


  <joint name="camera_joint" type="fixed">
    <origin xyz="0 0 0.225" rpy="0 0 0" />
    <parent link="gripper_pole_vertical"/>
    <child link="camera" />  
    <axis xyz="0 0 1"/> 
  </joint>

  <link name="camera">
      <xacro:default_inertial_cuboid mass="0.01" x_length="0.05" y_length="0.05" z_length="0.05" x="0" y="0" z="0"/>
      <collision>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <geometry>
          <box size="0.05 0.05 0.05"/>
        </geometry>
      </collision>

      <visual>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <geometry>
          <box size="0.05 0.05 0.05"/>
        </geometry>
      </visual>
    </link>

    <joint name="camera_optical_joint" type="fixed">
      <parent link="camera"/>
      <child link="camera_optical"/>
      <origin xyz="0.0 0.0 0.0" rpy="${-pi/2} 0.0 ${-pi/2}"/>
    </joint>

    <link name="camera_optical"></link>

